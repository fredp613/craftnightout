    <section id="events" class="features">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="container-fluid">
                        <div class="row">
						{{#if featuredevents }}
								<div class="row">
									<div class="col-lg-12 text-center">
										<div class="section-heading">
											<h2>upcomming events. register now.</h2>
											<p class="text-muted">select an event below to view additional details</p>
											<hr>
										</div>
									</div>
								</div>
							{{#each featuredevents}}
								<div class="col-md-6">
								   <div class="feature-item">
									   <a class="text-muted" href="/events/{{_id}}">
											<h2>{{title}}</h2>
									   </a>
									   <a href="/events/{{_id}}">
											<p class="text-muted">{{description}}</p>
									   </a>
											<p class="text-muted">{{date}}</p>
											<p class="text-muted">{{locationname}}</p>
											<p class="text-muted">{{address}}</p>
									</div>
								</div>
							{{/each}}
						{{else}}
								<div class="row">
									<div class="col-lg-12 text-center">
										<div class="section-heading">
											<h2>~ Events ~</h2>
											<h1>coming soon!</h1>
											<p class="text-muted">Please sign up for information on new events, fun news and all things crafty!</p>
											<div class="container">
												<div class="row">
													<div class="col-md-12">
													<h3 class="col-md-5" style="text-align:right;">January 15 <br> January 26 <br> January 29</h3>
													  <div style="height:100px"><a href="http://www.stampcraftlove.com/craft-night-out-.html" class="col-md-3 btn btn-primary" target="_blank" style="top:40%">Reserve your spot</a></div>
													</div>
												</div>
											</div>
											<hr>
										</div>
										 <form id="frmSubscribe" action="/subcribers/create"  class="form-inline">
											<input type="email" id="txtEmail" class="input-lg form-control" placeholder="enter email" required />
											<input type="submit" id="btnSubmit" class="btn btn-lg btn-success" value="Subscribe" />
										</form>
									</div>
								</div>
						{{/if}}
                        </div>
                </div>
            </div>
    </section>
    
